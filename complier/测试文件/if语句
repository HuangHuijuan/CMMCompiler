/**
 * IF-ELSE 测试
 * IF-ELSE 测试 主要是：
 * 1.条件判断
 * 2.语句执行
 * 3.IF-ELSE 嵌套
 */

int a;
a=1;
if(a * 5 == 5)
{
  real r ;
  r = 2.0;
  if(r>1.0)
  {
	write(r);
  }
}
else
{
   a=5;
   write(a);
} 

//
int aa ;
aa =3;
if(aa < 4)
	{
	if(2 < aa)
		{if(aa <> 3)
			{write(aa);}
		 else
			write(aa-2);
		}
	}


/*output:
2.0
1
*///:~